{
  "name": "lambdaneo-api",
  "version": "1.0.0",
  "description": "studying project",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node src/index.ts",
    "dev": "rimraf dist && tsc-watch --onSuccess \"npm run watch:server\"",
    "watch:server": "nodemon --watch \"./src\" --exec \"ts-node ./src/index.ts\"",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\""
  },
  "author": "Me and my team",
  "license": "MIT",
  "dependencies": {
    "@swc/core": "^1.10.4",
    "@types/cookie-parser": "^1.4.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "http-status": "^2.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.3",
    "winston": "^3.17.0",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.3",
    "@types/nodemailer": "^6.4.16",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.11.0"
  }
}
